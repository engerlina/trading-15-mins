# Kronos Trading System Environment Variables
# Copy this file to .env and fill in your values

# ===========================================
# BINANCE (for historical data collection)
# ===========================================
# Optional - public OHLCV endpoints work without authentication
# Only needed if you want higher rate limits
BINANCE_API_KEY=

# ===========================================
# HYPERLIQUID (for live/paper trading)
# ===========================================
# Ethereum wallet-based authentication
HYPERLIQUID_WALLET_ADDRESS=
HYPERLIQUID_API_WALLET_ADDRESS=
HYPERLIQUID_PRIVATE_KEY=
HYPERLIQUID_NETWORK=mainnet

